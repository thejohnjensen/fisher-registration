@model IEnumerable<Fish_Registration.Models.Project>

@{
    ViewData["Title"] = "VesselRatings";
}

<section class="rate_vessel_top">
  <h1>Vessel Ratings</h1>
  <h2>Sea Dragon</h2>
  <h3>Current Captain: Som Nang</h3>
  <h3>Previous Captain: John Galt</h3>
</section>

<div class="div_profile_image">
    <p>Flag History: </p>
    <div class="flag_group">
      <img src="~/images/thai-flag-small.jpg" />
      <p>(Current)</p>
    </div>
    <div class="flag_group">
      <img src="~/images/japanese-flag-small.jpg" style="border: solid 1px;"/>
      <p>(1996 - 2003)</p>
    </div>
</div>

<div class="list_container">
  <ul class="rate_vessel_options">
      <li>PAYROLL INTEGRITY</li>
      <li class="worker_tag">(3 workers)</li>
      <li>
          <div class="stars stars-example-fontawesome">
                  <select id="example-fontawesome" name="rating" autocomplete="off">
                    <option value="1">1</option>
                    <option value="2">2</option>
                    <option value="3">3</option>
                    <option value="4" selected="4">4</option>
                    <option value="5">5</option>
                  </select>
          </div>
      </li>
      
      <li>WORK ENVIRONMENT</li>
      <li class="worker_tag">(7 workers)</li>
      <li>
          <div class="stars stars-example-fontawesome">
                  <select id="example-fontawesome2" name="rating" autocomplete="off">
                    <option value="1">1</option>
                    <option value="2">2</option>
                    <option value="3" selected="3">3</option>
                    <option value="4">4</option>
                    <option value="5">5</option>
                  </select>
          </div>
      </li>
  </ul>
</div>

<div class="container">
<div class="row">
<div class="col-sm-12">
<h3>User Comment Example</h3>
</div><!-- /col-sm-12 -->
</div><!-- /row -->
